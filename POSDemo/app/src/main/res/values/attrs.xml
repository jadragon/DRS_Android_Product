<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SquareFrameLayout">
        <attr name="ratio" format="float" />
        <attr name="ratio_by_heigh" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="MultiTextColor">
        <attr name="text_start" format="integer"/>
        <attr name="text_end" format="integer"/>
        <attr name="text_color" format="integer"/>
    </declare-styleable>
</resources>